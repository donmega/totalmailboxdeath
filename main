from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.chrome.options import Options
from selenium.webdriver import ActionChains
from selenium.webdriver.support import expected_conditions as EC
from selenium.webdriver.chrome.service import Service
from selenium.webdriver.support.ui import WebDriverWait
import keyboard
import time
import numpy
from numpy import loadtxt

#          >>>>> >>>>><<<<< <<<<<

# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ definitions раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- 

driver = webdriver.Chrome()
get_url = driver.current_url
wait = WebDriverWait(driver,5)
shnooemail = "pasteemail@example.me"  ## replacc.e th uh target  email :3
shnoofirstname = "Example John" ## replace this witdh anything, doesn't matter !
shnoolastname = "Example Smith" ## replace this witdh anything, doesn't matter !

# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- 

def tmd_con():
    WebDriverWait(driver,3)
    print("The current url is:"+str(get_url))

# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- 
## ╦І┬░Рђб*РЂђРъи shnooie for all email element variants
def tmd_Shnooie_email(sn_Email):
    try: 
        driver.implicitly_wait(1)
        sn_Email_element = driver.find_element(By.NAME, sn_Email)
        sn_Email_element.send_keys(shnooemail)
        if sn_Email.is_displayed():
            time.sleep(1)
            print("Рѓі╦џ­ЊєЕРЎА­Њєф ╩џ╔ъ sucessfully found name element Ръи ", sn_Email)
    except:
            pass
    finally:
        time.sleep(1)
        webdriver.ActionChains(driver).send_keys(Keys.ENTER).perform()
        WebDriverWait(driver,2)

# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- 
## ╦І┬░Рђб*РЂђРъи shnooie for all first name element variants
def tmd_Shnooie_firstName(sn_firstName):
    try: 
        driver.implicitly_wait(1)
        sn_Email_element = driver.find_element(By.NAME, sn_firstName)
        sn_Email_element.send_keys(shnoofirstname)
    except:
            pass
    finally:
        time.sleep(1)


 # -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ --    

emaillist = ["fields[email]", "email", "EMAIL", "contact[email]", "et_pb_signup_email", "email_address", "Email"]

def tmd_Snorrey_email():
    for i in emaillist:
        tmd_Shnooie_email(i)
    

# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ --  

firstNamelist = ["fname", "name", "first_name", "firstname", "et_pb_signup_firstname"]

def tmd_Snorrey_firstName():
    for i in firstNamelist:
        tmd_Shnooie_firstName(i)

# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ --  

surNamelist = ["lname", "lastname", "last_name"]

def tmd_Snorrey_surName():
    for i in surNamelist:
        tmd_Shnooie_surName(i)

# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ --  
my_file = open("list.txt", "r") 
data = my_file.read() 
websitesList = data.split("\n")
# -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- раЃ Рџўраѓ Рџў ╦џ Рџў раѓ Рџў раЃ -- 
def tmd_Website(website):
    driver.get(website)
    tmd_Snorrey_email()
    tmd_Snorrey_firstName()
    time.sleep(3)

def tmd_Snorrey_web():
    for i in websitesList:
        tmd_Website(i)

tmd_Snorrey_web()
