from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.chrome.options import Options
from selenium.webdriver import ActionChains
from selenium.webdriver.support import expected_conditions as EC
from selenium.webdriver.chrome.service import Service

import time

#          >>>>> >>>>><<<<< <<<<<


driver = webdriver.Chrome()
email = "pasteemail@example.me"  ## replacc.e th uh email :3
first_name = "Example John" ## replace this with anything, doesn't matter
last_name = "Example Smith" ## replace this with anything, doesn't matter


def tmd_shnooie_method2():
    user_email = driver.find_element(By.NAME, "email")
    time.sleep(2)
    user_email.send_keys(email)
    webdriver.ActionChains(driver).send_keys(Keys.ENTER).perform()
    time.sleep(5)

def tmd_shnooie_method3():
    time.sleep(2)

# -- ˋ°•*⁀➷ email name elements variants °♡
# --
    try:
        driver.find_element(By.NAME, "email").send_keys(email)
    except:
        print("・:〃♡ no name element ꒰ email ꒱ found, searching...") ## element name ꒰ email ꒱
    try:
        driver.find_element(By.NAME, "EMAIL").send_keys(email) ## element name ꒰ EMAIL ꒱
    except:
        print("・:〃♡ no name element ꒰ EMAIL ꒱ found, searching...")
    try:
        driver.find_element(By.NAME, "email_address").send_keys(email) ## element name ꒰ email_address ꒱
    except:
        print("・:〃♡ no name element ꒰ email_address ꒱ found, searching...")
    
# -- ˋ°•*⁀➷ firstname name element variants °♡ 
# -- 
    try:
        driver.find_element(by.NAME, "fields[first_name]").send_keys(first_name) ## element name ꒰ "fields[first_name] ꒱
    except:
        print("・:〃♡ no name element ꒰ fields[first_name] ꒱ found, searching...")
    try:
        driver.find_element(by.ID, "mc-FNAME").send_keys(first_name) ## element name ꒰ mc-FNAME ꒱
    except:
        print("・:〃♡ no ID element ꒰ mc-FNAME ꒱ found, searching...")

# -- ˋ°•*⁀➷ lastname name element variants °♡
# -- 
    try:
        driver.find_element(by.ID, "mc-LNAME").send_keys(first_name) ## element ID ꒰ mc-LNAME ꒱
    except:
        print("・:〃♡ no ID element ꒰ mc-LNAME ꒱ found, searching...")

    # -- ♡ done searching ♡
    time.sleep(3)
    webdriver.ActionChains(driver).send_keys(Keys.ENTER).perform()
    time.sleep(6)


